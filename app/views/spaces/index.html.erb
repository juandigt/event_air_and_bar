<h1>Spaces#index</h1>
<p>Find me in app/views/spaces/index.html.erb</p>

<%= form_tag(spaces_path, method: :get) do %>
  <%= text_field_tag :address, params[:address] %>
  <%= submit_tag 'Search' %>
<% end %>





<h1>List of spaces</h1>
<% @spaces.each do |space| %>
  <h2><%= space.name %></h2>
  <p><%= space.address %></p>
<% end %>
